name: "streaming_paraformer"
platform: "ensemble"
max_batch_size: 128 #MAX_BATCH

input [
  {
    name: "wav"
    data_type: TYPE_FP32
    dims: [-1]
  }
]

output [
  {
    name: "enc"
    data_type: TYPE_FP32
    dims: [-1, 512]
  },
  {
    name: "enc_len"
    data_type: TYPE_INT32
    dims: [1]
  },
  {
    name: "alphas"
    data_type: TYPE_FP32
    dims: [-1]
  }
]

ensemble_scheduling {
  step [
    {
      model_name: "feature_extractor"
      model_version: -1
      input_map {
        key: "wav"
        value: "wav"
      }
      output_map {
        key: "speech"
        value: "speech"
      }
    },
    {
      model_name: "lfr_cmvn_pe"
      model_version: -1
      input_map {
        key: "chunk_xs"
        value: "speech"
      }
      output_map {
        key: "chunk_xs_out"
        value: "chunk_xs_out"
      }
      output_map {
        key: "chunk_xs_out_len"
        value: "chunk_xs_out_len"
      }
    },
    {
      model_name: "encoder"
      model_version: -1
      input_map {
        key: "speech"
        value: "chunk_xs_out"
      }
      input_map {
        key: "speech_lengths"
        value: "chunk_xs_out_len"
      }
      output_map {
        key: "enc"
        value: "enc"
      }
      output_map {
        key: "enc_len"
        value: "enc_len"
      }
      output_map {
        key: "alphas"
        value: "alphas"
      }
    }
  ]
}
